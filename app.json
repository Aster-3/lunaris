{
  "expo": {
    "name": "lunaris",
    "slug": "lunaris",
    "version": "1.0.1",
    "web": {
      "favicon": "./assets/lunaris_logo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-font", "expo-sqlite"],
    "orientation": "portrait",
    "icon": "./assets/lunaris_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.onyx99.lunaris"
    },
    "extra": {
      "eas": {
        "projectId": "2ba01d95-9c65-476c-a9f1-b9c110aa090a"
      }
    }
  }
}
